{
  "name": "@rowanmanning/validate-project",
  "version": "3.1.3",
  "description": "A tool to validate my coding projects",
  "author": "Rowan Manning (https://rowanmanning.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/rowanmanning/validate-project.git"
  },
  "homepage": "https://github.com/rowanmanning/validate-project",
  "bugs": "https://github.com/rowanmanning/validate-project/issues",
  "license": "MIT",
  "engines": {
    "node": "18.x || 20.x",
    "npm": "8.x || 9.x"
  },
  "scripts": {
    "verify": "npm run verify:eslint",
    "verify:eslint": "eslint .",
    "project:verify": "npx --yes @rowanmanning/validate-project@3 --type git node-library",
    "project:fix": "npx --yes @rowanmanning/validate-project@3 --type git node-library --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "dot-prop": "^8.0.0",
    "glob": "^8.0.1",
    "jsdom": "^23.1.0",
    "marked": "^5.0.2",
    "semver": "^7.3.5",
    "yaml": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@commitlint/config-conventional": "^18.4.2",
    "@rowanmanning/eslint-config": "^8.0.0",
    "eslint": "^8.9.0",
    "husky": "^8.0.1"
  },
  "main": "lib/runner.js",
  "bin": "bin/validate.js"
}